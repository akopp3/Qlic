<?xml version="1.0" encoding="utf-8"?>
<com.redbooth.WelcomePageLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/welcome_page"
    tools:background="@color/setup1">

    <RelativeLayout
        android:id="@+id/rocket"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingTop="16dp"
        app:view_behavior=".AnimationLaunch"
        android:clipToPadding="false">

    </RelativeLayout>
    <ImageView
        android:id="@+id/rocketimg"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:paddingTop="32dp"
        android:layout_below="@+id/rocket"
        android:src="@drawable/rocket"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"/>
    <TextView
        style="@style/text_title"
        android:id="@+id/title"
        android:layout_above="@+id/subtitle"
        android:text="Envoyer"/>

    <TextView
        style="@style/text_subtitle"
        android:id="@+id/subtitle"
        android:layout_alignParentBottom="true"
        android:text="Share your contact \ninformation simply and elegantly"/>

</com.redbooth.WelcomePageLayout>
