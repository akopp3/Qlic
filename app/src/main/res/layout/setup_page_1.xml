<?xml version="1.0" encoding="utf-8"?>
<com.redbooth.WelcomePageLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/welcome_page"
    tools:background="@color/setup1">

    <ImageView
        android:id="@+id/rocketimg"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/rocket" />

    <TextView
        android:id="@+id/title"
        style="@style/text_title"
        android:layout_above="@+id/subtitle"
        android:text="Envoyer" />

    <TextView
        android:id="@+id/subtitle"
        style="@style/text_subtitle"
        android:layout_alignParentBottom="true"
        android:text="Share your contact \ninformation simply and elegantly" />

</com.redbooth.WelcomePageLayout>
